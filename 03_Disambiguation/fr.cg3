DELIMITERS = "." ;

LIST DET = DET ;
LIST PUNCT = PUNCT ;
LIST PRON = PRON ;
LIST ADV = ADV ;
LIST ADJ = ADJ ;
LIST VERB = VERB ;
LIST NOUN = NOUN;

SECTION

REMOVE DET IF (1C PUNCT) ;
REMOVE PRON IF (NOT 1C PRON) ;
REMOVE ADV IF (NOT 1C VERB) ;
REMOVE ADJ IF (NOT 1C NOUN) ;
REMOVE NOUN IF (1C NOUN) ;
